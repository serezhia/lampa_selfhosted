# Domain (lampa.example.com)
DOMAIN=

# Base URL used by the server (https://lampa.example.com)
BASE_URL=

# Telegram Bot Token (get from @BotFather)
TELEGRAM_BOT_TOKEN=

# Telegram Bot Name (displayed on auth screen, e.g. @my_lampa_bot)
TELEGRAM_BOT_NAME=

# Admin phone numbers (comma-separated, e.g. +79001234567,+79007654321)
# Admins can manage notices and see all users
TELEGRAM_ADMIN_PHONES=

# Jackett API Key (get from Jackett web interface after first start)
# Leave empty to disable Jackett proxy
JACKETT_API_KEY=

# Email for Let's Encrypt
LETSENCRYPT_EMAIL=

# HTTPS enabled (true/false)
USE_SSL=true
